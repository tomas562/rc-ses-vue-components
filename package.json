{
  "name": "@registrucentras/rc-ses-vue-components",
  "author": "VĮ REGISTRŲ CENTRAS",
  "version": "0.1.3",
  "description": "RC SES Vue.js komponentų biblioteka",
  "type": "module",
  "private": false,
  "main": "./dist/registrucentras-rc-ses-vue-components.cjs",
  "module": "./dist/registrucentras-rc-ses-vue-components.js",
  "types": "./dist/library/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/library/index.d.ts",
      "import": "./dist/registrucentras-rc-ses-vue-components.js",
      "require": "./dist/registrucentras-rc-ses-vue-components.cjs",
      "default": "./dist/registrucentras-rc-ses-vue-components.cjs"
    },
    "./vuetify/*": "./dist/styles/vuetify/*",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*",
    "public/*",
    "src/*",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vue-tsc --p ./tsconfig.lib.json && vite build --config vite.config.build.mts",
    "clean:cache": "rimraf node_modules/.vite && rimraf node_modules/.cache",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "preview-storybook": "http-server ./storybook-static",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@storybook/docs-tools": "^8.2.6",
    "@storybook/vue3": "^8.2.6",
    "@vee-validate/rules": "4.13.2",
