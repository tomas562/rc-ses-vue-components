    "js-cookie": "^3.0.5",
    "maska": "^3.0.0-beta4",
    "uuid": "^10.0.0",
    "vee-validate": "^4.13.2",
    "vite-plugin-css-injected-by-js": "^3.5.1",
    "yup": "^1.4.0"
  },
  "peerDependencies": {
    "@vuepic/vue-datepicker": "8.8.0",
